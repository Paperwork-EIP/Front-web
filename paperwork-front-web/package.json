    "axios": "^0.27.2",
    "framer-motion": "^6.3.11",
    "jest-styled-components": "^7.0.8",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^3.11.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "swagger-ui": "^4.12.0",
