{
  "name": "paperwork-front-web",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/icons": "^2.0.2",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/node": "^12.20.55",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "axios": "^0.27.2",
    "framer-motion": "^6.5.1",
    "i18next": "^22.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-styled-components": "^7.0.8",
    "react-calendar": "^3.9.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^12.3.1",
    "react-icons": "^4.8.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.4.0",
    "react-toastify": "^9.1.3",
    "react-vertical-timeline-component": "^3.6.0",
    "sass": "^1.58.3",
    "swagger-ui": "^4.12.0",
    "typescript": "^4.7.3",
    "universal-cookie": "^4.0.4",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --silent --coverage=true",
    "test:coverage:log": "jest --coverage=true",
    "test:all": "jest --watchAll --coverage=true",
    "eject": "react-scripts eject"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "/node_modules/",
      "\\.pnp\\.[^\\/]+$"
    ],
    "coveragePathIgnorePatterns": [
      "src/components/index.tsx"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.15.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^27.0.2",
    "@types/react-calendar": "^3.5.2",
    "@types/react-modal": "^3.13.1",
    "@types/react-vertical-timeline-component": "^3.3.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0"
  }
}
